# x
[stepper_b]
step_pin: PF0
dir_pin: PF1
enable_pin: !PD7
microsteps: 16
gear_ratio: 100:5

# y
[stepper_s]
step_pin: PF6
dir_pin: !PF7
enable_pin: !PF2
microsteps: 16
gear_ratio: 100:5

# z
[stepper_a]
step_pin: PL3
dir_pin: PL1
enable_pin: !PK0
microsteps: 16
gear_ratio: 100:5

[fan]
pin: PH6

[mcu]
serial: /dev/serial/by-id/usb-1a86_USB2.0-Serial-if00-port0

[printer]
kinematics: forward
# shoulder
l1: 300.0
# arm
l2: 300.0
# bed location
x: 0.0
y: 0.0
l0: 60.0
max_velocity: 300
max_accel: 3000

[homing_override]
gcode:
set_position_x: 0
set_position_y: 0
set_position_z: 0
